{
  "name": "sideral",
  "version": "1.0.0",
  "description": "A lightweight javascript game engine framework",
  "main": "src/index.js",
  "scripts": {
    "start": "node --harmony ./app.js",
    "build": "./node_modules/.bin/eslint src && yarn test && ./node_modules/.bin/babel src --out-dir lib",
    "build-features": "./node_modules/.bin/webpack -d --config config/webpack.config.js",
    "build-features-dev": "./node_modules/.bin/webpack -d --config config/webpack.config.js --watch --progress --profile --colors",
    "build-dev": "./node_modules/.bin/babel src --watch --out-dir lib",
    "test": "node_modules/.bin/karma start config/karma.conf.prod.js",
    "test-dev": "node_modules/.bin/karma start config/karma.conf.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-republic/sideral.git"
  },
  "keywords": [
    "sideral",
    "game",
    "framework",
    "js",
    "javascript"
  ],
  "author": "JS-Republic",
  "license": "GPL-3.0",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "bugs": {
    "url": "https://github.com/js-republic/sideral/issues"
  },
  "homepage": "https://github.com/js-republic/sideral#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chokidar": "^1.6.1",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.10.2",
    "eslint-plugin-react": "latest",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "karma-source-map-support": "^1.2.0",
    "karma-webpack": "^1.8.0",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.3.2",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "http": "^0.0.0",
    "material-ui": "^0.16.4",
    "morgan": "^1.7.0",
    "path": "^0.12.7",
    "pug": "^2.0.0-beta6",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hexagon": "^1.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "serve-favicon": "^2.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
