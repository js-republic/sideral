{
  "name": "sideral",
  "version": "1.0.0",
  "description": "A lightweight javascript game engine framework",
  "main": "src/index.js",
  "scripts": {
    "start": "node --harmony ./app.js",
    "start:dev": "./node_modules/.bin/webpack-dev-server --config config/webpack.config.js --hot --progress --colors --profile",
    "build": "yarn install && yarn run build-src && yarn run build-webpack",
    "build-src": "yarn run eslint && yarn test && ./node_modules/.bin/babel src --out-dir lib",
    "build-src-dev": "./node_modules/.bin/babel src --watch --out-dir lib",
    "build-webpack": "./node_modules/.bin/webpack -d --config config/webpack.config.js",
    "build-webpack-dev": "./node_modules/.bin/webpack -d --config config/webpack.config.js --watch --progress --profile --colors",
    "test": "node_modules/.bin/karma start config/karma.conf.prod.js",
    "test-dev": "node_modules/.bin/karma start config/karma.conf.dev.js",
    "eslint": "./node_modules/.bin/eslint src",
    "generate-doc": "./node_modules/.bin/jsdoc -c ./config/jsdoc.json",
    "doc": "typedoc --out ./ts-doc ./ --module amd",
    "doc:serve": "simplehttpserver ./ts-doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-republic/sideral.git"
  },
  "keywords": [
    "sideral",
    "game",
    "framework",
    "js",
    "javascript"
  ],
  "author": "JS-Republic",
  "license": "GPL-3.0",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "bugs": {
    "url": "https://github.com/js-republic/sideral/issues"
  },
  "homepage": "https://github.com/js-republic/sideral#readme",
  "devDependencies": {
    "@types/p2": "^0.7.29",
    "@types/pixi.js": "^4.4.4",
    "@types/signals": "^0.0.17",
    "awesome-typescript-loader": "^3.1.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chokidar": "^1.6.1",
    "cz-conventional-changelog": "^1.2.0",
    "docdash": "^0.4.0",
    "eslint": "^3.10.2",
    "eslint-plugin-react": "latest",
    "ify-loader": "^1.0.3",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.5.2",
    "jsdoc": "^3.4.3",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "karma-source-map-support": "^1.2.0",
    "karma-webpack": "^1.8.0",
    "typedoc": "^0.7.1",
    "typescript": "^2.3.2",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-eslint": "6",
    "babel-preset-stage-2": "^6.18.0",
    "body-parser": "^1.15.2",
    "brfs": "^1.4.3",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.3.2",
    "eslint": "^3.19.0",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "http": "^0.0.0",
    "material-ui": "^0.16.4",
    "morgan": "^1.7.0",
    "ncp": "^2.0.0",
    "p2": "^0.7.1",
    "path": "^0.12.7",
    "pixi-particles": "^2.1.5",
    "pixi.js": "^4.3.2",
    "pug": "^2.0.0-beta6",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "serve-favicon": "^2.3.0",
    "signals": "^1.0.0",
    "simplehttpserver": "^0.0.6",
    "transform-loader": "^0.2.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
